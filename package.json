    "@angular/platform-browser-dynamic": "^4.3.1",
    "@types/express": "^4.0.36",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/mime": "^1.3.1",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^2.2.7",
