    "@types/node": "^8.0.14",
    "@types/serve-static": "^1.7.31",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "@types/socket.io-redis": "^1.0.21",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "immutable": "^3.8.1",
    "js.clone": "^0.0.3",
    "json-loader": "^0.5.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.3",
    "node-sass": "^4.5.3",
