    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "5.4.2",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.12.3",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
