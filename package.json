    "typescript": "^2.4.2",
    "webpack": "3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.14",
    "body-parser": "~1.15.2",
    "validator": "~7.2.0",
    "q": "1.5.0",
    "mysql": "*",
    "utf8-bytes":"*"
  },
  "devDependencies": {
    "@types/webpack": "^3.0.4",
    "concurrently": "^3.5.0",
    "dotenv-cli": "^1.4.0",
